{
  "name": "vue-leaflet-sidepanel",
  "version": "1.2.0",
  "description": "Slide side panel plugin for vue-leaflet",
  "author": "cyclingbyte <cyclingbyte@web.de>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/vue-leaflet-sidepanel.cjs.js",
  "unpkg": "dist/vue-leaflet-sidepanel.umd.js",
  "module": "dist/vue-leaflet-sidepanel.es.js",
  "types": "dist/lib.d.ts",
  "style": "dist/vue-leaflet-sidepanel.css",
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cyclingbyte/vue-leaflet-sidepanel.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "vitest",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:check": "eslint . --max-warnings 0 --ignore-pattern .gitignore || (echo 'Linter failed' && exit 1)",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/ || (echo 'Code is not formatted' && exit 1)"
  },
  "dependencies": {
    "leaflet.sidepanel": "^1.2.3"
  },
  "peerDependencies": {
    "@vue-leaflet/vue-leaflet": "^0.10.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "leaflet": "^1.9.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^3.1.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.10"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "leaflet",
    "leafletjs",
    "vue-leaflet",
    "@vue-leaflet/vue-leaflet",
    "vue-leaflet-sidepanel",
    "vue-leaflet-sidebar",
    "leaflet-sidepanel",
    "leaflet-sidebar"
  ]
}
