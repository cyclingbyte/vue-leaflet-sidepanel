{
  "name": "vue-leaflet-sidepanel",
  "version": "1.1.1",
  "description": "Slide side panel plugin for vue-leaflet",
  "author": "cyclingbyte <cyclingbyte@web.de>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/vue-leaflet-sidepanel.cjs.js",
  "unpkg": "dist/vue-leaflet-sidepanel.umd.js",
  "module": "dist/vue-leaflet-sidepanel.es.js",
  "types": "dist/lib.d.ts",
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cyclingbyte/vue-leaflet-sidepanel.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "vitest",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:check": "eslint . --max-warnings 0 --ignore-pattern .gitignore || (echo 'Linter failed' && exit 1)",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/ || (echo 'Code is not formatted' && exit 1)"
  },
  "dependencies": {
    "leaflet.sidepanel": "^1.2.1"
  },
  "peerDependencies": {
    "@vue-leaflet/vue-leaflet": "^0.10.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-oxlint": "^0.11.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "leaflet": "^1.9.4",
    "prettier": "^3.2.5",
    "typescript": "^5.5.4",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^3.1.2",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.6"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "leaflet",
    "leafletjs",
    "vue-leaflet",
    "@vue-leaflet/vue-leaflet",
    "vue-leaflet-sidepanel",
    "vue-leaflet-sidebar",
    "leaflet-sidepanel",
    "leaflet-sidebar"
  ]
}
